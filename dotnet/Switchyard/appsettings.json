{
  "Switchyard": {
    "Server": {
      "HealthPort": 9081
    },
    "Transports": {
      "Grpc": {
        "Enabled": true,
        "Port": 50151
      },
      "Http": {
        "Enabled": true,
        "Port": 9080
      },
      "Mqtt": {
        "Enabled": false,
        "Broker": "tcp://localhost:1883",
        "Topic": "switchyard/#"
      }
    },
    "Interpreter": {
      "Backend": "openai",
      "OpenAI": {
        "ApiKey": "${OPENAI_API_KEY}",
        "TranscriptionModel": "gpt-4o-transcribe",
        "CompletionModel": "gpt-4o"
      },
      "Local": {
        "WhisperEndpoint": "http://localhost:8000/v1/audio/transcriptions",
        "WhisperType": "openai",
        "LlmEndpoint": "http://localhost:11434/api/generate",
        "LlmModel": "llama3",
        "VadFilter": false,
        "Language": ""
      }
    },
    "Tts": {
      "Enabled": false,
      "Backend": "piper",
      "Piper": {
        "Endpoint": "localhost:10200",
        "Endpoints": {
          "en": "localhost:10200",
          "fr": "localhost:10201"
        },
        "Voices": {
          "en": "en_US-lessac-medium",
          "fr": "fr_FR-siwis-medium",
          "es": "es_ES-mls_10246-low"
        }
      }
    },
    "Targets": {
      "homeassistant": {
        "Endpoint": "http://homeassistant.local:8123/api/services",
        "Protocol": "http",
        "Token": "${HA_TOKEN}"
      }
    },
    "Logging": {
      "Level": "info",
      "Format": "json"
    }
  }
}
